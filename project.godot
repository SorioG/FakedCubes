; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Faked Cubes"
config/version="1.0.0"
run/main_scene="res://scenes/boot.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.513726, 0.513726, 0.513726, 1)
boot_splash/image="res://assets/sprites/logo.png"
boot_splash/use_filter=false
config/icon="res://assets/sprites/bigplayer.png"
boot_splash/minimum_display_time=600

[audio]

buses/default_bus_layout="res://assets/audio_bus.tres"
driver/enable_input=true

[autoload]

LoadingScreen="*res://scenes/loading_screen.tscn"
Global="*res://assets/scripts/Global.gd"

[debug]

multirun/number_of_windows=2
multirun/add_custom_args=true
multirun/window_args=["--server", "--connect"]

[display]

window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/size/mode.debug=0

[editor_plugins]

enabled=PackedStringArray("res://addons/multirun/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "blue",
"res://scenes/": "green"
}

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://assets/ui_theme.tres"

